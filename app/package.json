  "private": true,
  "dependencies": {
    "@arizeai/components": "^0.9.21",
    "@arizeai/point-cloud": "^2.1.5",
    "@emotion/react": "^11.10.5",
    "@react-three/drei": "9.5.7",
    "@react-three/fiber": "8.0.12",
    "@types/react-table": "^7.7.14",
    "@types/recharts": "^1.8.24",
    "d3-scale-chromatic": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "date-fns": "^2.29.3",
    "normalize.css": "^8.0.1",
    "polished": "^4.2.2",
    "react": "18.2",
    "react-dom": "18.2",
    "react-relay": "^14.1.0",
    "react-resizable-panels": "^0.0.36",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "react-table": "^7.8.0",
    "recharts": "^2.4.3",
    "relay-runtime": "^14.1.0",
    "shade": "^0.1.1",
    "three": "0.139",
    "three-stdlib": "^2.17.3",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/react-dom": "^18.0.8",
    "@types/react-relay": "^14.1.2",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "graphql": "^15.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "relay-compiler": "^14.1.0",
    "typescript": "4.8"
  },
  "scripts": {
    "prebuild": "rm -rf ../src/phoenix/server/static",
    "build": "npm run build:static & npm run build:relay && npm run build:ts",
    "build:static": "cp -r ./static ../src/phoenix/server",
    "build:ts": "node ./esbuild.config.mjs",
    "build:relay": "relay-compiler",
    "watch": "./esbuild.config.mjs dev",
    "dev": "npm run dev:server:mnist & npm run build:static && npm run watch",
    "dev:server:mnist": "python3 -m phoenix.server.main fixture fashion_mnist",
    "dev:server:sentiment": "python3 -m phoenix.server.main fixture sentiment_classification_language_drift",
    "dev:server:ner": "python3 -m phoenix.server.main fixture ner_token_drift",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "prettier": "prettier --write './src/**/*'",
    "prettier:check": "prettier --check './src/**/*'"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js}": "eslint --cache --fix",
    "src/*.{js,ts,tsx,md}": "prettier --write --ignore-unknown"
  }
}
