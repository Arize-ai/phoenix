{
  "name": "phoenix-ui",
  "version": "0.0.1",
  "author": "mikyo@arize.com",
  "url": "https://github.com/Arize-ai/phoenix/issues",
  "license": "None",
  "private": true,
  "dependencies": {
    "@arizeai/components": "0.9.2",
    "@arizeai/point-cloud": "^2.1.1",
    "@emotion/react": "^11.10.5",
    "@react-three/drei": "9.5.7",
    "@react-three/fiber": "8.0.12",
    "polished": "^4.2.2",
    "react": "18.2",
    "react-dom": "18.2",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0",
    "shade": "^0.1.1",
    "three": "0.139",
    "three-stdlib": "^2.17.3"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.8",
    "@types/react-relay": "^14.1.2",
    "esbuild": "^0.15.13",
    "graphql": "^15.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "relay-compiler": "^14.1.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "prebuild": "rm -rf ../src/phoenix/server/static",
    "build": "npm run build:static & npm run build:relay && npm run build:ts",
    "build:static": "cp -r ./static ../src/phoenix/server",
    "build:ts": "node ./esbuild.config.mjs",
    "build:relay": "relay-compiler",
    "watch": "./esbuild.config.mjs dev",
    "dev": "npm run dev:server & npm run build:static && npm run watch",
    "dev:server": "python3 ../src/phoenix/server/main.py --primary=primary --reference=reference",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write './src/**/*'",
    "prettier:check": "prettier --check './src/**/*'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
