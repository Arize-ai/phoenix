{
  "name": "@arizeai/phoenix-insight",
  "version": "0.0.1",
  "description": "A CLI for Phoenix data analysis with AI agents",
  "main": "dist/src/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "phoenix-insight": "dist/src/cli.js"
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "pnpm run clean",
    "build": "tsc --build tsconfig.json tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "postbuild": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest --typecheck"
  },
  "keywords": [
    "phoenix",
    "cli",
    "insight",
    "arize",
    "llmops",
    "ai"
  ],
  "author": "oss@arize.com",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Arize-ai/phoenix/tree/main/js/packages/phoenix-insight",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arize-ai/phoenix.git"
  },
  "bugs": {
    "url": "https://github.com/Arize-ai/phoenix/issues"
  },
  "dependencies": {
    "@arizeai/phoenix-client": "workspace:*",
    "ai": "^6.0.0",
    "@ai-sdk/anthropic": "^3.0.0",
    "commander": "^12.0.0",
    "just-bash": "^2.2.0",
    "bash-tool": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9",
    "rimraf": "^5.0.10",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.21.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  }
}
