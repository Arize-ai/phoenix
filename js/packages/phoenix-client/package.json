{
  "name": "@arizeai/phoenix-client",
  "version": "0.0.1",
  "description": "A client for the Phoenix API",
  "main": "dist/src/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/src/index.js"
    },
    "./prompts": {
      "import": "./dist/esm/prompts/index.js",
      "require": "./dist/src/prompts/index.js"
    },
    "./experimental": {
      "import": "./dist/esm/experimental/index.js",
      "require": "./dist/src/experimental/index.js"
    },
    "./utils/*": {
      "import": "./dist/esm/utils/*.js",
      "require": "./dist/src/utils/*.js"
    },
    "./types/*": {
      "import": "./dist/esm/types/*.d.ts",
      "require": "./dist/src/types/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "pnpm run clean && pnpm run generate",
    "generate": "openapi-typescript --empty-objects-unknown=true ../../../schemas/openapi.json -o ./src/__generated__/api/v1.ts",
    "build": "tsc --build tsconfig.json tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "postbuild": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json && rimraf dist/test dist/examples",
    "type:check": "tsc --noEmit",
    "test": "vitest --typecheck"
  },
  "keywords": [],
  "author": "",
  "license": "ELv2",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.35.0",
    "@types/node": "^20.14.11",
    "openapi-typescript": "^7.4.1",
    "tsx": "^4.19.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "openapi-fetch": "^0.12.2",
    "remeda": "^2.20.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "ai": "^4.1.0",
    "openai": "^4.77.0"
  }
}
