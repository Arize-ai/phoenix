{
  "name": "@arizeai/phoenix-cli",
  "version": "0.5.0",
  "description": "A command-line interface for Phoenix",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "phoenix-cli": "build/index.js",
    "px": "build/index.js"
  },
  "files": [
    "build",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "pnpm run clean",
    "build": "tsc && tsc-alias && chmod 755 build/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "phoenix",
    "cli",
    "arize",
    "llmops"
  ],
  "author": "oss@arize.com",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Arize-ai/phoenix/tree/main/js/packages/phoenix-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arize-ai/phoenix.git"
  },
  "bugs": {
    "url": "https://github.com/Arize-ai/phoenix/issues"
  },
  "dependencies": {
    "@arizeai/phoenix-client": "workspace:*",
    "@arizeai/phoenix-config": "workspace:*",
    "@arizeai/openinference-semantic-conventions": "^1.1.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "rimraf": "^5.0.10",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  }
}
