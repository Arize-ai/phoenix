{
  "name": "@arizeai/phoenix-evals",
  "version": "0.0.0",
  "description": "An evaluation library for LLM applications",
  "main": "dist/src/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.json tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "postbuild": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json && rimraf dist/test",
    "type:check": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ELv2"
}
